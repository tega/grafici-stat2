\begin_inset Formula \begin{equation}
P(E)=\frac{\#\text{ esiti in }E}{\#\text{ esiti in }\Omega}=\frac{\#\text{ esiti favorevoli}}{\#\text{ esiti totali}}.\label{formula esiti equiprobabili}\end{equation}

